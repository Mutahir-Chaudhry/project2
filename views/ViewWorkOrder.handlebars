<div class="container-fluid">
  <div class="container">
    <h2 class="justify-content-center">
      {{msg}}
    </h2>
    <h4>Work Order Info:</h4>
    <ul id="WorkOrder-List" class="list-group">
      <div data-id={{this.id}} class="container list-group-items">
        <div class="row">
          <strong>#</strong> {{this.id}}
        </div>
        <br>
        <div class="row">
          <strong>Client Name</strong>: {{this.Name}}
        </div>
        <br>
        <div class="row">
          <strong>W.O Description</strong>: {{this.Description}}
        </div>
        <br>
        <div class="row">
          <strong>Completed</strong>: {{this.Completed}}
        </div>
      </div>
    </ul>
    <br>
    <br>
    <br>
    <h4>Time Sheet Info:</h4>
    <ul id="TimeSheet-List" class="list-group">
      <div data-id={{this.id}} class="container list-group-items">
        <div class="row">
          <strong>Employee</strong>: 
        </div>
        <br>
        <div class="row">
          <strong>Start TIme</strong>: 
        </div>
        <br>
        <div class="row">
          <strong>End Time</strong>:
        </div>
        <br>
        <div class="row">
          <strong>Description</strong>:
        </div>
      </div>
    </ul>
    <br>
    <br>
    {{!-- This button has a class of home we will use to take us back to --}}
    <div class="row">
      <div class="col">
        {{!-- Display Index --}}
        <form action="/">
          <button class="btn btn-primary home">Home

          </button>
        </form>
      </div>
      <div class="col">
        {{!-- Change value of BOOLEAN to 1 for true, refresh WorkOrder, display Index --}}
        <button class="btn btn-primary complete">Complete This Work Order</button>
      </div>
      <div class="col">
        {{!-- Submit Button --}}
        <form action="/">
          <button class="btn btn-success submit">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>